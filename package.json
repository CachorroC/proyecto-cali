{
  "name": "proyecto-cali",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "styles:tcm": "tcm src",
    "sass": "sass --no-source-map src:src && tcm src",
    "devbuild": "sass --no-source-map src:src && tcm src && eslint --fix . && next build",
    "build": "next build --webpack",
    "dev": "sass --no-source-map src:src && tcm src && URL=beta.rsasesorjuridico.com NEXTAUTH_URL=https://beta.rsasesorjuridico.com next dev --turbopack",
    "start": "next start",
    "db:migrate": "prisma migrate dev --name init --create-only && prisma generate",
    "db:push": "prisma db push",
    "json-to-prisma": "json-to-prisma-schema-convertor convert --inputPath='./prisma/schema.json' --outputPath='./prisma/new/schema.prisma'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.38.0",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@stylistic/eslint-plugin": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "jiti": "^2.6.1",
    "material-symbols": "^0.36.4",
    "mongodb": "^6.20.0",
    "next": "16.0.0",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sass": "^1.93.2",
    "typed-css-modules": "^0.9.1",
    "typed-scss-modules": "^8.1.1",
    "typescript-eslint": "^8.46.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20.19.24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.4",
    "typescript": "^5.9.3"
  }
}
